{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "esnext",
      "esnext.asynciterable"
    ],
    "module": "ESNext",
    "strict": true,
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "types": [
      "jest",
      "node"
    ]
  }
}